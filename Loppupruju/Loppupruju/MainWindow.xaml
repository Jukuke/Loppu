<Window x:Class="Loppupruju.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Loppupruju"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="comboBox1" HorizontalAlignment="Left" Height="25" Margin="54,78,0,0" VerticalAlignment="Top" Width="125" SelectionChanged="ComboBox_SelectionChanged"/>
        <TextBox x:Name="UnitPrice" HorizontalAlignment="Left" Height="25" Margin="195,78,0,0" TextWrapping="Wrap" Text="UnitPrice" VerticalAlignment="Top" Width="113" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="Quantity" HorizontalAlignment="Left" Height="25" Margin="340,78,0,0" TextWrapping="Wrap" Text="Quantity" VerticalAlignment="Top" Width="113"/>
        <TextBox x:Name="Discount" HorizontalAlignment="Left" Height="25" Margin="483,78,0,0" TextWrapping="Wrap" Text="Discount" VerticalAlignment="Top" Width="113"/>

    </Grid>
</Window>
